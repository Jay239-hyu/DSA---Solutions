nums = [1,1,2]

if not nums:
    print("Array is Empty") #Handle the empty array

i = 0  # Track the position of unique elements 


for j in range(1 , len(nums)):
    if nums[i] != nums[j]:
        i += 1
        nums[i] = nums[j]

print(f"The Total unique elements in given array: {i+1}")
print(nums)

    



